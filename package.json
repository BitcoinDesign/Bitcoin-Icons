{
  "name": "@bitcoin-design/bitcoin-icons",
  "version": "0.1.8",
  "description": "About",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build-outline && npm run build-filled && npm run build-react && npm run build-vue",
    "build-react": "node ./scripts/build.js react",
    "build-vue": "node ./scripts/build.js vue",
    "build-outline": "rimraf ./outline ./optimized/outline && svgo --config=svgo.outline.js -f ./svg/outline -o ./optimized/outline && cp -R ./optimized/outline ./outline",
    "build-filled": "rimraf ./filled ./optimized/filled && svgo --config=svgo.filled.js -f ./svg/filled -o ./optimized/filled && cp -R ./optimized/filled ./filled",
    "test-react": "cp ./data/icons.json ./tests/react-test/src/icons.json && cd tests/react-test && npm i && npm run start",
    "test-vue": "cp ./data/icons.json ./tests/vue-test/src/icons.json && cd tests/vue-test && npm i && npm run serve",
    "test-react-native": "cp ./data/icons.json ./tests/react-native-test/icons.json && cd tests/react-native-test && npm i && npm run start"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-react-jsx": "^7.12.12",
    "@svgr/core": "^5.5.0",
    "@vue/compiler-dom": "^3.0.5",
    "camelcase": "^6.0.0",
    "rimraf": "^3.0.2",
    "svgo": "^2.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BitcoinDesign/Bitcoin-Icons.git"
  },
  "keywords": [
    "bitcoin",
    "icons"
  ],
  "author": "Bitcoin Design Community",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BitcoinDesign/Bitcoin-Icons/issues"
  },
  "homepage": "https://github.com/BitcoinDesign/Bitcoin-Icons#readme"
}
